{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
      "width": 300,
    "height": 300,
    "title": {
      "text": "Relationship between Education and Climate Change",
      "subtitle": "Source: Eurostat",
      "fontSize": 13,
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "#000000",
      "subtitleColor": "#000000"
    },
    
    "data": {
      "url": "https://raw.githubusercontent.com/karmun2710/karmun2710.github.io/main/Emission2.csv",
      "format": {
          "type": "csv"}
      },
    "mark": "point",
    "encoding": {
      "color": {
          "field": "Educational attainment",
          "legend": "",
          "type": "quantitative",
          "scale": {"scheme": "viridis"}},
      "x": {"field": "Educational attainment","type": "quantitative", "title": "Population with Upper Secondary Level of Education/Higher (%)"},
      "y": {"field": "Air emissions", "type": "quantitative", "title": "Greenhouse Gas Emissions"},
      
      "tooltip": [
          {
            "field": "Country", 
            "type": "nominal", 
            "title": "Country"},
          {
            "field": "Educational attainment", 
            "type": "quantitative",
            "title": "Educational attainment"},
          {
            "field": "Population", 
            "type": "quantitative",
            "title": "Population (millions)"}
        ]
    }
  }
  